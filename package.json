{
  "name": "react-redux-webpack-typescript-template",
  "version": "0.1.0",
  "description": "This is template for easy start for react redux typescript application",
  "author": "Kamil Bysiec",
  "license": "MIT",
  "main": "app.js",
  "scripts": {
    "dev": "webpack-dev-server --config config/webpack.dev.config.js --open",
    "dev:dashboard": "webpack-dashboard -m -- webpack-dev-server --config config/webpack.dev.config.js --env.dashboard=true --open",
    "build": "webpack --config config/webpack.prod.config.js",
    "lint:scripts": "eslint src/** && tslint --project ./tsconfig.json",
    "lint:styles": "stylelint src/**/*.{css,scss,less}",
    "lint:js": "eslint src/**",
    "lint:ts": "tslint --project ./tsconfig.json",
    "lint:scss": "stylelint src/**/*.scss",
    "lint:less": "stylelint src/**/*.less",
    "lint:css": "stylelint src/**/*.css",
    "pretty": "prettier --write src/**/*",
    "test": "jest --config jest.json",
    "test:details": "jest --config jest.json --coverage --no-cache",
    "test:watch": "jest --config jest.json --watch",
    "test:update": "jest --config jest.json -u"
  },
  "dependencies": {
