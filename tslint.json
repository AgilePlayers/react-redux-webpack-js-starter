{
  "extends": ["tslint-config-airbnb"],
  "linterOptions": {
    "exclude": ["./config/**.ts"]
  },
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "ter-indent": [
      true,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "import-name": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ]
  }
}
